<div class="blue accent-1">
  <% @articles.each do |article| %>
    <%= link_to article_path(article.slug) do %>
      <div class="row card">
        <div class="col s3 offset-s1 black-text">
          <h5><%=article.title%><h5>
        </div>
      <% end%>
      </div>
    <%end%>
</div>
<div class="row">
  <div class="col s1 black card">
    <%= link_to "< Previous", category_path(page: @page - 1) %>
  </div>
  <div class="col s1 offset-s10 black card">
    <%= link_to "Next >", category_path(page: @page + 1) %>
  </div>
</div>
